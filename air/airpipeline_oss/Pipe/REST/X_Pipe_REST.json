{
    "imports": [
        "github.com/project-flogo/flow",
        "git.tibco.com/git/product/ipaas/wi-contrib.git/contributions/General/activity/log",
        "github.com/TIBCOSoftware/ModelOps/activity/objectmaker",
        "github.com/TIBCOSoftware/ModelOps/activity/httpclient",
        "github.com/project-flogo/contrib/activity/actreturn",
        "github.com/project-flogo/contrib/function/string",
        "github.com/TIBCOSoftware/F1_Function/functions/f1",
        "git.tibco.com/git/product/ipaas/wi-contrib.git/function/boolean"
    ],
    "name": "X_Pipe_REST",
    "description": "",
    "version": "1.0.0",
    "type": "flogo:app",
    "appModel": "1.1.1",
    "feVersion": "2.9.0",
    "triggers": [],
    "resources": [
        {
            "id": "flow:Pipe",
            "data": {
                "name": "Pipe",
                "description": "",
                "links": [
                    {
                        "id": 1,
                        "from": "Log",
                        "to": "ConstructRequests",
                        "type": "default"
                    },
                    {
                        "id": 2,
                        "from": "ConstructRequests",
                        "to": "SendRequests",
                        "type": "default"
                    },
                    {
                        "id": 3,
                        "from": "SendRequests",
                        "to": "Return",
                        "type": "default"
                    }
                ],
                "tasks": [
                    {
                        "id": "Log",
                        "name": "Log",
                        "description": "Simple Log Message Activity",
                        "activity": {
                            "ref": "#log",
                            "input": {
                                "Log Level": "=$property[\"Logging.LogLevel\"]",
                                "flowInfo": false,
                                "message": "=string.concat(\"########## Inference:REST ########## gateway : \", $flow.gateway, \", reading : \", string.tostring($flow.reading), \", enriched : \", string.tostring($flow.enriched))"
                            }
                        }
                    },
                    {
                        "id": "ConstructRequests",
                        "name": "ConstructRequests",
                        "description": "Make an New Object",
                        "activity": {
                            "ref": "#objectmaker",
                            "settings": {
                                "schemaFromfile": false,
                                "sample": "",
                                "dataSample": {
                                    "metadata": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{\"Methods\":{\"type\":\"array\",\"items\":{\"type\":\"string\"}},\"URLs\":{\"type\":\"array\",\"items\":{\"type\":\"string\"}},\"Bodies\":{\"type\":\"array\",\"items\":{\"type\":\"string\"}}}}",
                                    "value": "",
                                    "fe_metadata": "{\"Methods\":[\"\"],\"URLs\":[\"1\"], \"Bodies\":[\"\"]}"
                                },
                                "defaultValue": ""
                            },
                            "input": {
                                "ObjectDataMapping": {
                                    "mapping": {
                                        "Methods": "=f1.json2object($property[\"REST.Methods\"])",
                                        "URLs": "=f1.json2object($property[\"REST.URLs\"])",
                                        "Bodies": "=f1.json2object($property[\"REST.Bodies\"])"
                                    }
                                }
                            },
                            "schemas": {
                                "input": {
                                    "ObjectDataMapping": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{\"Methods\":{\"type\":\"array\",\"items\":{\"type\":\"string\"}},\"URLs\":{\"type\":\"array\",\"items\":{\"type\":\"string\"}},\"Bodies\":{\"type\":\"array\",\"items\":{\"type\":\"string\"}}}}",
                                        "fe_metadata": "{\"Methods\":[\"\"],\"URLs\":[\"1\"],\"Bodies\":[\"\"]}"
                                    }
                                },
                                "output": {
                                    "ObjectOut": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{\"Methods\":{\"type\":\"array\",\"items\":{\"type\":\"string\"}},\"URLs\":{\"type\":\"array\",\"items\":{\"type\":\"string\"}},\"Bodies\":{\"type\":\"array\",\"items\":{\"type\":\"string\"}}}}",
                                        "fe_metadata": "{\"Methods\":[\"\"],\"URLs\":[\"1\"],\"Bodies\":[\"\"]}"
                                    }
                                },
                                "settings": {
                                    "dataSample": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{\"Methods\":{\"type\":\"array\",\"items\":{\"type\":\"string\"}},\"URLs\":{\"type\":\"array\",\"items\":{\"type\":\"string\"}},\"Bodies\":{\"type\":\"array\",\"items\":{\"type\":\"string\"}}}}",
                                        "fe_metadata": "{\"Methods\":[\"\"],\"URLs\":[\"1\"], \"Bodies\":[\"\"]}"
                                    }
                                }
                            }
                        }
                    },
                    {
                        "id": "SendRequests",
                        "name": "SendRequests",
                        "description": "This activity build docker image from Dockerfile",
                        "type": "iterator",
                        "settings": {
                            "iterate": "=$activity[ConstructRequests].ObjectOut.URLs",
                            "accumulate": false
                        },
                        "activity": {
                            "ref": "#httpclient",
                            "settings": {
                                "method": "GET",
                                "timeout": "1500",
                                "urlMappingString": "[]",
                                "urlMapping": "[]",
                                "leftToken": "$",
                                "rightToken": "$",
                                "variablesDef": "",
                                "httpHeaders": "[{\"Key\":\"Accept\",\"Value\":\"application/json\"},{\"Key\":\"Content-Type\",\"Value\":\"application/json-patch+json\"}]"
                            },
                            "input": {
                                "URL": "=$iteration[value]",
                                "Body": "=f1.airdataselector($flow.gateway, $flow.reading, $flow.enriched, $activity[ConstructRequests].ObjectOut.Bodies[$iteration[index]])",
                                "SkipCondition": "=boolean.false()"
                            },
                            "output": {
                                "Success": false
                            },
                            "schemas": {
                                "input": {
                                    "Variables": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{}}",
                                        "fe_metadata": "{}"
                                    }
                                },
                                "settings": {
                                    "urlMapping": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"array\",\"items\":{}}",
                                        "fe_metadata": "[]"
                                    },
                                    "httpHeaders": {
                                        "type": "json",
                                        "value": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"array\",\"items\":{\"type\":\"object\",\"properties\":{\"Key\":{\"type\":\"string\"},\"Value\":{\"type\":\"string\"}},\"required\":[\"Key\",\"Value\"]}}",
                                        "fe_metadata": "[{\"Key\":\"Accept\",\"Value\":\"application/json\"},{\"Key\":\"Content-Type\",\"Value\":\"application/json-patch+json\"}]"
                                    }
                                }
                            }
                        }
                    },
                    {
                        "id": "Return",
                        "name": "Return",
                        "description": "Simple Return Activity",
                        "activity": {
                            "ref": "#actreturn"
                        }
                    }
                ],
                "metadata": {
                    "input": [
                        {
                            "name": "gateway",
                            "type": "string"
                        },
                        {
                            "name": "reading",
                            "type": "object",
                            "schema": {
                                "type": "json",
                                "value": "{\"id\":{\"type\":\"string\"},\"origin\":{\"type\":\"number\"},\"device\":{\"type\":\"string\"},\"name\":{\"type\":\"string\"},\"value\":{\"type\":\"string\"},\"valueType\":{\"type\":\"string\"},\"mediaType\":{\"type\":\"string\"}}"
                            }
                        },
                        {
                            "name": "enriched",
                            "type": "array",
                            "schema": {
                                "type": "json",
                                "value": "{\"type\":\"object\",\"properties\":{\"producer\":{\"type\":\"string\"},\"consumer\":{\"type\":\"string\"},\"name\":{\"type\":\"string\"},\"value\":{\"type\":\"string\"},\"type\":{\"type\":\"string\"}}}"
                            }
                        }
                    ],
                    "output": [],
                    "fe_metadata": {
                        "input": "{\"$schema\":\"http://json-schema.org/draft-04/schema#\",\"type\":\"object\",\"properties\":{\"gateway\":{\"type\":\"string\"},\"reading\":{\"type\":\"object\",\"properties\":{\"id\":{\"type\":\"string\"},\"origin\":{\"type\":\"number\"},\"device\":{\"type\":\"string\"},\"name\":{\"type\":\"string\"},\"value\":{\"type\":\"string\"},\"valueType\":{\"type\":\"string\"},\"mediaType\":{\"type\":\"string\"}}},\"enriched\":{\"type\":\"array\",\"items\":{\"type\":\"object\",\"properties\":{\"producer\":{\"type\":\"string\"},\"consumer\":{\"type\":\"string\"},\"name\":{\"type\":\"string\"},\"value\":{\"type\":\"string\"},\"type\":{\"type\":\"string\"}}}}}}"
                    }
                }
            }
        }
    ],
    "properties": [
        {
            "name": "Logging.LogLevel",
            "type": "string",
            "value": "DEBUG"
        },
        {
            "name": "REST.Conditions",
            "type": "string",
            "value": "[]"
        },
        {
            "name": "REST.Producer",
            "type": "string",
            "value": "Inference.REST"
        },
        {
            "name": "REST.Consumer",
            "type": "string",
            "value": "*"
        },
        {
            "name": "REST.URLs",
            "type": "string",
            "value": "[]"
        },
        {
            "name": "REST.Methods",
            "type": "string",
            "value": "GET"
        },
        {
            "name": "REST.Bodies",
            "type": "string",
            "value": "{}"
        }
    ],
    "connections": {},
    "contrib": "",
